% WWU theme
% https://github.com/anishathalye/gemini

% ====================
% Definitions
% ====================

\definecolor{lightgray}{RGB}{245, 246, 250}
\definecolor{blue}{RGB}{64, 115, 158}
\definecolor{darkblue}{RGB}{39, 60, 117}
\definecolor{lightblue}{RGB}{102, 127, 164}
\definecolor{wwugreen}{RGB}{101, 105, 80}
\definecolor{wwubrown}{RGB}{121, 98, 73}
\definecolor{wwured}{RGB}{150, 20, 0}
\definecolor{wwutan}{RGB}{199, 174, 134}

% ====================
% Theme
% ====================

% Basic colors
\setbeamercolor{palette primary}{fg=black,bg=white}
\setbeamercolor{palette secondary}{fg=black,bg=white}
\setbeamercolor{palette tertiary}{bg=black,fg=white}
\setbeamercolor{palette quaternary}{fg=black,bg=white}
\setbeamercolor{structure}{fg=wwugreen}

% Headline
\setbeamercolor{headline}{fg=wwugreen,bg=white}
\setbeamercolor{headline rule}{bg=wwugreen}

% Block
\setbeamercolor{block title}{fg=wwubrown,bg=white}
\setbeamercolor{block separator}{bg=wwugreen}
\setbeamercolor{block body}{fg=black,bg=white}

% Alert Block
\setbeamercolor{block alerted title}{fg=wwubrown,bg=wwutan}
\setbeamercolor{block alerted separator}{bg=black}
\setbeamercolor{block alerted body}{fg=black,bg=wwutan}

% Example Block
\setbeamercolor{block example title}{fg=wwured,bg=lightgray}
\setbeamercolor{block example separator}{bg=black}
\setbeamercolor{block example body}{fg=black,bg=lightgray}

% Heading
\setbeamercolor{heading}{fg=black}

% Itemize
\setbeamercolor{item}{fg=wwubrown}

% Bibliography
\setbeamercolor{bibliography item}{fg=black}
\setbeamercolor{bibliography entry author}{fg=black}
\setbeamercolor{bibliography entry title}{fg=black}
\setbeamercolor{bibliography entry location}{fg=black}
\setbeamercolor{bibliography entry note}{fg=black}
\setbeamertemplate{bibliography entry article}{}
\setbeamertemplate{bibliography entry title}{}
\setbeamertemplate{bibliography entry location}{}
\setbeamertemplate{bibliography entry note}{}
